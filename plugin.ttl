@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<https://github.com/YimRakHee>
    a foaf:Person ;
    foaf:name "YimRakHee" ;
    foaf:homepage <https://github.com/YimRakHee> .

<https://github.com/YimRakHee/CleanAmpForBass>
    a lv2:Plugin , lv2:AmplifierPlugin ;
    doap:name "CleanAmpForBass" ;
    doap:license <http://opensource.org/licenses/isc> ;
    doap:maintainer <https://github.com/YimRakHee> ;
    lv2:project <https://github.com/YimRakHee/CleanAmpForBass> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:port [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 0 ;
        lv2:symbol "in" ;
        lv2:name "In"
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 1 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:ControlPort ,
            lv2:InputPort ;
        lv2:index 2 ;
        lv2:symbol "hpf_freq" ;
        lv2:name "HPF Frequency" ;
        lv2:default 30.0 ;
        lv2:minimum 20.0 ;
        lv2:maximum 200.0 ;
        units:unit units:hz ;
        lv2:portProperty lv2:logarithmic
    ] , [
        a lv2:ControlPort ,
            lv2:InputPort ;
        lv2:index 3 ;
        lv2:symbol "bass" ;
        lv2:name "Bass (80Hz)" ;
        lv2:default 0.0 ;
        lv2:minimum -15.0 ;
        lv2:maximum 15.0 ;
        units:unit units:db
    ] , [
        a lv2:ControlPort ,
            lv2:InputPort ;
        lv2:index 4 ;
        lv2:symbol "mid_freq" ;
        lv2:name "Mid Frequency" ;
        lv2:default 500.0 ;
        lv2:minimum 200.0 ;
        lv2:maximum 2500.0 ;
        units:unit units:hz ;
        lv2:portProperty lv2:logarithmic
    ] , [
        a lv2:ControlPort ,
            lv2:InputPort ;
        lv2:index 5 ;
        lv2:symbol "mid_gain" ;
        lv2:name "Mid Gain" ;
        lv2:default 0.0 ;
        lv2:minimum -15.0 ;
        lv2:maximum 15.0 ;
        units:unit units:db
    ] , [
        a lv2:ControlPort ,
            lv2:InputPort ;
        lv2:index 6 ;
        lv2:symbol "treble" ;
        lv2:name "Treble (3kHz)" ;
        lv2:default 0.0 ;
        lv2:minimum -15.0 ;
        lv2:maximum 15.0 ;
        units:unit units:db
    ] , [
        a lv2:ControlPort ,
            lv2:InputPort ;
        lv2:index 7 ;
        lv2:symbol "lpf_freq" ;
        lv2:name "LPF Frequency" ;
        lv2:default 5000.0 ;
        lv2:minimum 1000.0 ;
        lv2:maximum 15000.0 ;
        units:unit units:hz ;
        lv2:portProperty lv2:logarithmic
    ] , [
        a lv2:ControlPort ,
            lv2:InputPort ;
        lv2:index 8 ;
        lv2:symbol "master_gain" ;
        lv2:name "Master Gain" ;
        lv2:default 0.0 ;
        lv2:minimum -20.0 ;
        lv2:maximum 20.0 ;
        units:unit units:db
    ] .
